<div class="container">
<h1>All Patients</h1>
<table class="table table-striped">
  <tr>
    <th>ID</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Age</th>
  </tr>
  <% @patients.each do |patient| %>
    <tr>
      <td> <%= patient.id %> </td>
      <td> <%= patient.first_name %> </td>
      <td> <%= patient.last_name %> </td>
      <td> <%= patient.email %> </td>
      <td> <%= patient.age %> </td>
    </tr>
  <% end %>
</table>
